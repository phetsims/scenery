      "kite": "KITE/main",
      "dot": "DOT/main",
      "axon": "AXON/main",
      "phetCore": "PHET_CORE/main"
    },
    "finalizeJS": "scenery.Util.polyfillRequestAnimationFrame();"
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "globals": {
      "himalaya": false,
      "sceneryLog": false,
      "he": false
    }
  }
}