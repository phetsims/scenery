{
  "name": "scenery",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/scenery.git"
  },
  "devDependencies": {
    "grunt": "~1.0.0"
  },
  "phet": {
    "generatedUnitTests": true,
    "requirejsNamespace": "SCENERY",
    "buildStandalone": true,
    "isJSOnly": true,
    "requiresLodash": true,
    "requiresJQuery": true,
    "assignGlobals": {
